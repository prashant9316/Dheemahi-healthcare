                                        <h3><%= courses[i].courseName %></h3>
                                        <h6><%= courses[i].rating %> 
                                            <ul class="doctor-icon">
                                                <li><a href="#"><i class="fa fa-star"></i><a></li>
                                                <li><a href="#"><i class="fa fa-star"></i><a></li>
                                                <li><a href="#"><i class="fa fa-star"></i><a></li>
                                                <li><a href="#"><i class="fa fa-star"></i><a></li>
                                                <li><a href="#"><i class="fa fa-star"></i><a></li>

                                            </ul>
                                        </h6>
                                    </div>
                                    <div class="doctor-text text-center">
                                        <p><%= courses[i].courseSubTitle %></p>
                                        <a href="<%= courses[i].courseLink %>" class="genric-btn info">View Course</a>
                                        
                                    </div>
                                </div>
                            </div>
                        </div>
                    
                  <% } %>

                   
